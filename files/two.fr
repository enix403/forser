struct LoginRequest {
    username: string,
    password: string,
}

struct AuthKeys {
    accessToken: string,
    refreshToken: string
}

struct UserProfile {
    name: string,
    profilePictureId: string?,
}

struct Role {
    slug: string,
    grantLevel: int
}

struct LoginResult {
    keys: AuthKeys,
    profile: UserProfile,
    lastLoggedIn: int,
    roles: [Role]
}
