struct LoginRequest {
    username: string,
    password: string,
    keep_logged_in: bool 
}

struct Role {
    id: string,
    permissions: [string]  
}

struct User {
    id: int,
    name: string,
    username: string,
    last_logged_in: int,
    roles: [Role]
}

struct LoginReply {
    success: bool,
    fail_reason: string,
    user: User
}